{
  "name": "hypnos",
  "version": "1.0.0",
  "description": "An app to test GraphQL calls on RESTful APIs, via Apollo.",
  "main": "electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "parcel -p 3000 ./index.html --out-dir build",
    "react-build": "parcel build ./index.html --out-dir build --public-url ./",
    "start": "npm run react-build && npm run start-electron",
    "start-electron": "electron .",
    "electron-build": "electron-builder",
    "clean-build": "rm -rf build/ .cache dist/",
    "build": "npm run clean-build && npm run react-build && npm run electron-build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Apollo",
    "Apollo-Link-Rest",
    "GraphQL",
    "REST API",
    "API",
    "Query",
    "Mutation"
  ],
  "author": "Dillon Garrett, Sophie Nye & Will Robinson",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "parcel-bundler": "^1.12.3"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-rest": "^0.7.3",
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6"
  }
}
